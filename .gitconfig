[user]
	name =
	email =
[core]
	editor = nvim

[alias]
        st = status
        sts = status -sb
        aa = add -A
        cm = commit -m
        cma = commit -am
        co = checkout
        cob = checkout -b
        br = branch
        bd = branch -d
        mrg = merge
        mrga = merge --abort
        lp = log --pretty
        res = reset --hard HEAD
        rev = revert -m 1 HEAD
